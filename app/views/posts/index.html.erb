<div class="postbox">
	<h1>Posts</h1>

	<h2><%= @posts.count %>
	<ul>

		<% @posts.each do |post| %>

			<li>
				<div><%= raw(post.image_link) %></div>
				<h1><%= post.title %></h1> <h2><%= post.question_one %></h2> <h2><%= post.question_two %></h2> <span> <%= link_to "Edit", edit_post_path(post) %> | <%= link_to "Destroy", post, method: :delete, data: {confirm: "Are you sure?" } %> </span> 
			</li>
		<% end %>


	</ul>
</div>

